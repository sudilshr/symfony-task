{% extends '::base.html.twig' %}
{% block body %}
<br>
<a href="{{ path('test_homepage') }}" class="btn btn-default"><span class="glyphicon glyphicon-menu-left"></span></a>
	<h1>Create Projects</h1>
	<div class="row">
		<div class="col-lg-5">
			{{ form_start(form) }}
			
			<div class="form-group">
				{{ form_label(form.title) }}
				{{ form_widget(form.title)}}
			</div>
			<div class="form-group">
				{{ form_label(form.completed) }}
				{{ form_widget(form.completed)}}
			</div>
			<div class="form-group">
				{{ form_label(form.due_date) }}
				{{ form_widget(form.due_date)}}
			</div>
			{{ form_end(form) }}
		</div>
	</div>
{% endblock %}